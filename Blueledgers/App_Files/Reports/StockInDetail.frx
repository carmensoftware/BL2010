      <TableDataSource Name="Table" Alias="rpd" DataType="System.Int32" Enabled="true" SelectCommand="DECLARE&#13;&#10;	@FDATE DATE = @p_FDate,&#13;&#10;	@TDATE DATE = @p_TDate,&#13;&#10;	@FCat NVARCHAR(20) = @p_FCat,&#13;&#10;	@TCat NVARCHAR(20) = @p_TCat,&#13;&#10;	@FSCat nvarchar(20) = @p_FSCat,&#13;&#10;	@TSCat nvarchar(20) = @p_TSCat,&#13;&#10;	@FIG nvarchar(20) = @p_FIG,&#13;&#10;	@TIG nvarchar(20) = @p_TIG,&#13;&#10;	@FProduct  NVARCHAR(20)=@p_FProduct,&#13;&#10;	@TProduct  NVARCHAR(20)=@p_TProduct,&#13;&#10;	@FLocation NVARCHAR(20)=@p_FLocation,&#13;&#10;	@TLocation NVARCHAR(20)=@p_TLocation,&#13;&#10;	@Status NVARCHAR(20)=@p_Status&#13;&#10;&#13;&#10;IF ISNULL(@FDATE, '') = ''&#13;&#10;	SET @FDATE = (SELECT MIN (CreateDate) FROM [IN].StockIn)&#13;&#10;&#13;&#10;IF ISNULL(@TDATE, '') = ''&#13;&#10;	SET @TDATE = (SELECT MAX (CreateDate) FROM [IN].StockIn)&#13;&#10;&#13;&#10;IF ISNULL(@FCAT, '') = ''&#13;&#10;	SET @FCAT = (SELECT	MIN (CategoryCode) FROM [IN].vProdCatList)&#13;&#10;&#13;&#10;IF ISNULL(@TCAT, '') = ''&#13;&#10;	SET @TCAT = (SELECT	MAX (CategoryCode) FROM	[IN].vProdCatList)&#13;&#10;&#13;&#10;IF ISNULL(@FSCAT, '') = ''&#13;&#10;	SET @FSCAT = (SELECT	MIN (SubCategoryCode) FROM [IN].vProdCatList)&#13;&#10;&#13;&#10;IF ISNULL(@TSCAT, '') = ''&#13;&#10;	SET @TSCAT = (SELECT	MAX (SubCategoryCode) FROM	[IN].vProdCatList)&#13;&#10;&#13;&#10;IF ISNULL(@FIG, '') = ''&#13;&#10;	SET @FIG = (SELECT	MIN (ItemGroupCode) FROM [IN].vProdCatList)&#13;&#10;&#13;&#10;IF ISNULL(@TIG, '') = ''&#13;&#10;	SET @TIG = (SELECT	MAX (ItemGroupCode) FROM [IN].vProdCatList)&#13;&#10;&#13;&#10;IF ISNULL(@FProduct, '') = ''&#13;&#10;	SET @FProduct = (SELECT	MIN (SKU) FROM [IN].StockIn si, [IN].StockInDt sidt WHERE si.RefId = sidt.Id AND CAST(CreateDate AS DATE) &lt;= @TDATE)&#13;&#10;	&#13;&#10;IF ISNULL(@TProduct, '')  = ''&#13;&#10;	SET @TProduct = (SELECT	MAX (SKU) FROM [IN].StockIn si, [IN].StockInDt sidt WHERE si.RefId = sidt.Id AND CAST(CreateDate AS DATE) &lt;= @TDATE)&#13;&#10;&#13;&#10;&#13;&#10;IF ISNULL(@FLOCATION, '') = ''&#13;&#10;	SET @FLOCATION = (SELECT MIN (StoreId)	FROM [IN].StockIn si, [IN].StockInDt sidt WHERE si.RefId = sidt.Id AND CAST(CreateDate AS DATE) &lt;= @TDATE)&#13;&#10;&#13;&#10;IF ISNULL(@TLOCATION, '') = ''&#13;&#10;	SET @TLOCATION = (SELECT MAX (StoreId) FROM [IN].StockIn si, [IN].StockInDt sidt WHERE si.RefId = sidt.Id AND CAST(CreateDate AS DATE) &lt;= @TDATE)&#13;&#10;&#13;&#10;-- Filter product by Category, Sub-Category and Item-Group&#13;&#10;DECLARE @Product TABLE(&#13;&#10;	ProductCode nvarchar(20),&#13;&#10;	ProductDesc1 nvarchar(400),&#13;&#10;	ProductDesc2 nvarchar(400),&#13;&#10;	ItemGroupCode nvarchar(20)&#13;&#10;)&#13;&#10;&#13;&#10;INSERT INTO @Product&#13;&#10;EXEC RPT.GetProductByItemGroup @FCat, @TCat, @FSCat, @TSCat, @FIG, @TIG, @FProduct, @TProduct&#13;&#10;&#13;&#10;SET @FProduct = (SELECT MIN(ProductCode) as ProductCode FROM @Product)&#13;&#10;SET @TProduct = (SELECT MAX(ProductCode) as ProductCode FROM @Product)&#13;&#10;&#13;&#10;&#13;&#10;SELECT &#13;&#10;	si.*,&#13;&#10;	sidt.*,&#13;&#10;	l.LocationCode,&#13;&#10;	l.LocationName,&#13;&#10;	p.ProductCode,&#13;&#10;	p.ProductDesc1,&#13;&#10;	p.ProductDesc2,&#13;&#10;	adj.AdjCode,&#13;&#10;	adj.AdjName&#13;&#10;FROM &#13;&#10;	[IN].StockIn si&#13;&#10;	JOIN [IN].StockInDt sidt &#13;&#10;		ON sidt.Id = si.RefId&#13;&#10;	LEFT JOIN [IN].StoreLocation l&#13;&#10;		ON l.LocationCode = sidt.StoreId&#13;&#10;	LEFT JOIN [IN].Product p&#13;&#10;		ON p.ProductCode = sidt.SKU&#13;&#10;	LEFT JOIN [IN].AdjType adj&#13;&#10;		ON AdjId = si.[Type]&#13;&#10;WHERE&#13;&#10;	CAST(si.CreateDate AS DATE) BETWEEN @FDate AND @TDate&#13;&#10;	AND sidt.SKU BETWEEN @FProduct AND @TProduct&#13;&#10;	AND sidt.StoreId BETWEEN @FLocation AND @TLocation&#13;&#10;	AND si.[Status] =  (CASE WHEN ISNULL(@Status, '') = '' THEN si.[Status]	ELSE @Status END)&#13;&#10;ORDER BY&#13;&#10;	si.RefId,&#13;&#10;	sidt.RefId">
