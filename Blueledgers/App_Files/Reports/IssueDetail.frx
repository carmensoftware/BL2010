        Report.SetParameterValue(&quot;p_FLocationFrom&quot;, null);
      else
        Report.SetParameterValue(&quot;p_FLocationFrom&quot;, ComboBox11.Text.Split(' ')[0].ToString());
      
      if (ComboBox12.SelectedIndex == 0)
