     private void _StartReport(object sender, EventArgs e)
    {
          GetComboBox();
    }
    
    
    private void GetComboBox()
    {
      string DatabaseName=Report.Dictionary.Connections[0].ConnectionString.ToString();
      SqlConnection cnn = new SqlConnection();
      string connetionString = &quot;Data Source=.\\SQLEXPRESS;Initial Catalog=&quot;+DatabaseName+&quot; &quot;;
      connetionString += &quot;;User ID=sa;Password=sa@genex&quot;;
      cnn = new SqlConnection(connetionString);
      try
      {
        cnn.Open();

      }
      catch (Exception ex)
      {
        cnn.Close();
        string ErrorMess = ex.ToString();
      }
      string strSQL = &quot;&quot;;
      strSQL =  &quot;Select  ProductCode+' '+ProductDesc1 AS Productcode &quot;;
      strSQL += &quot;FROM [IN].product  &quot;;
      strSQL += &quot; UNION SELECT 'ZZZZ All ProductCode' &quot;;
      strSQL +=&quot;ORDER  BY ProductCode&quot;;
      SqlCommand myCommand = new SqlCommand(strSQL, cnn);
      SqlDataAdapter da = new SqlDataAdapter(myCommand);
      DataSet ds =new DataSet();
      da.Fill(ds);

      for (int i=0;i&lt; ds.Tables[0].Rows.Count;i++)
      {
		     ComboBox4.Items.Add(ds.Tables[0].Rows[i][0].ToString());
      }
      
      ComboBox4.SelectedIndex=ComboBox4.Items.Count-1;
      
    }

    private void btnOk_Click(object sender, EventArgs e)
    {

      Report.SetParameterValue(&quot;p_FDate&quot;, Convert.ToDateTime(ComboBox5.Text.ToString()));
      Report.SetParameterValue(&quot;p_Flocation&quot;, ComboBox1.Text.Split(' ')[0].ToString());
      Report.SetParameterValue(&quot;p_Tlocation&quot;, ComboBox2.Text.Split(' ')[0].ToString());
      Report.SetParameterValue(&quot;p_Fproduct&quot;, ComboBox3.Text.Split(' ')[0].ToString());
      Report.SetParameterValue(&quot;p_Tproduct&quot;, ComboBox4.Text.Split(' ')[0].ToString());
      if (CheckBox1.Checked==true)
      {
        Report.SetParameterValue(&quot;p_All&quot;,0);
        Report.SetParameterValue(&quot;p_pshow&quot;,&quot;Show Only OnHand&quot;);
      }
      else
      {
        Report.SetParameterValue(&quot;p_All&quot;,1);
        Report.SetParameterValue(&quot;p_pshow&quot;,&quot;Show All&quot;);
      }
      
      
      
    }



  }
}
